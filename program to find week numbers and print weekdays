week_number = int(input("Enter a week number (1-7): "))
week_days = {
    1: "monday",
    2: "tuesday",
    3: "wednesday",
    4: "thursday",  
    5: "friday",
    6: "saturday",
    7: "sunday"
}

if 1 <= week_number <= 7:
    print("The day is:", week_days[week_number]) 
else:
    print("Not a valid week number")
